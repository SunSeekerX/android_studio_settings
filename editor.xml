<application>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="2" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="12" />
    <option name="FONT_FAMILY" value="Hack Nerd Font" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc; *.java; *.kt; *.gradle" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>